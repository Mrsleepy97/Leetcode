int largestAltitude(vector<int>& gain) {
        
        int element=0;

        int maxi=INT_MIN;

        for(int i=0;i<gain.size();i++)
        {
            
            maxi=max(element, maxi);
            element+=gain[i];

        }
        
        maxi=max(element,maxi);

        return maxi;
    }
